<Project>
  <ItemGroup>
    <_MicrosoftIdentityUICandidate Include="$(MSBuildThisFileDirectory)..\staticwebassets\V4\**" />
    <StaticWebAsset Include="@(_MicrosoftIdentityUICandidate->'%(FullPath)')">
      <SourceType>Package</SourceType>
      <SourceId>Microsoft.AspNetCore.Identity.UI</SourceId>
      <ContentRoot>$(MSBuildThisProjectDirectory)..\staticwebassets\V4</ContentRoot>
      <BasePath>Identity</BasePath>
      <RelativePath>$([System.String]::Copy('%(RecursiveDir)%(FileName)%(Extension)').Replace('\', '/').TrimStart('/'))</RelativePath>
      <AssetKind>All</AssetKind>
      <AssetMode>All</AssetMode>
      <AssetRole>Primary</AssetRole>
      <RelatedAsset></RelatedAsset>
      <AssetTraitName></AssetTraitName>
      <AssetTraitValue></AssetTraitValue>
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
      <OriginalItemSpec>@(_MicrosoftIdentityUICandidate->'%(FullPath)')</OriginalItemSpec>
    </StaticWebAsset>
  </ItemGroup>
</Project>
